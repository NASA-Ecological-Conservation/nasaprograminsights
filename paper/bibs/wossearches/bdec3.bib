@article{ WOS:000402398400015,
Author = {Davies, Gwendolyn E. and Calvin, Wendy M.},
Title = {Quantifying Iron Concentration in Local and Synthetic Acid Mine
   Drainage: A New Technique Using Handheld Field Spectrometers},
