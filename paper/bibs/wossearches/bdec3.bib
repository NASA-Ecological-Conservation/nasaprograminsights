   Waring, Richard H.; Hilker, Thomas, Oregon State Univ, Coll Forestry, Corvallis, OR 97331 USA.},
DOI = {10.1016/j.rse.2012.08.024},
ISSN = {0034-4257},
Keywords = {Ecoregions; Soil water holding capacity; Fertility index; MODIS leaf
   area index; Climate-driven models; Remote sensing; 3-PG forest growth
   model},
Keywords-Plus = {DAILY MINIMUM TEMPERATURE; CARBON SEQUESTRATION; PACIFIC-NORTHWEST;
