@article{ WOS:000387669600017,
Author = {Couture, John J. and Singh, Aditya and Rubert-Nason, Kennedy F. and
   Serbin, Shawn P. and Lindroth, Richard L. and Townsend, Philip A.},
Title = {Spectroscopic determination of ecologically relevant plant secondary
   metabolites},
Journal = {METHODS IN ECOLOGY AND EVOLUTION},
Year = {2016},
