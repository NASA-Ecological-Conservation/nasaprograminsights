@article{ WOS:000667707100047,
Author = {Huesca, Margarita and Ustin, Susan L. and Shapiro, Kristen D. and
   Boynton, Ryan and Thorne, James H.},
Title = {Detection of drought-induced blue oak mortality in the Sierra Nevada
   Mountains, California},
Journal = {ECOSPHERE},
