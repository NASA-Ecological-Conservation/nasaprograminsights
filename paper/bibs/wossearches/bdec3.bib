@article{ WOS:000301533100013,
Author = {Fordham, Damien A. and Akcakaya, H. Resit and Araujo, Miguel B. and
   Elith, Jane and Keith, David A. and Pearson, Richard and Auld, Tony D.
   and Mellin, Camille and Morgan, John W. and Regan, Tracey J. and Tozer,
   Mark and Watts, Michael J. and White, Matthew and Wintle, Brendan A. and
   Yates, Colin and Brook, Barry W.},
Title = {Plant extinction risk under climate change: are forecast range shifts
   alone a good indicator of species vulnerability to global warming?},
Journal = {GLOBAL CHANGE BIOLOGY},
Year = {2012},
Volume = {18},
Number = {4},
Pages = {1357-1371},
Month = {APR},
Abstract = {Models that couple habitat suitability with demographic processes offer
   a potentially improved approach for estimating spatial distributional
   shifts and extinction risk under climate change. Applying such an
   approach to five species of Australian plants with contrasting
   demographic traits, we show that: (i) predicted climate-driven changes
   in range area are sensitive to the underlying habitat model, regardless
   of whether demographic traits and their interaction with habitat patch
   configuration are modeled explicitly; and (ii) caution should be
   exercised when using predicted changes in total habitat suitability or
   geographic extent to infer extinction risk, because the relationship
   between these metrics is often weak. Measures of extinction risk, which
   quantify threats to population persistence, are particularly sensitive
   to life-history traits, such as recruitment response to fire, which
   explained approximately 60\% of the deviance in expected minimum
   abundance. Dispersal dynamics and habitat patch structure have the
   strongest influence on the amount of movement of the trailing and
   leading edge of the range margin, explaining roughly 40\% of modeled
   structural deviance. These results underscore the need to consider
   direct measures of extinction risk (population declines and other
   measures of stochastic viability), as well as measures of change in
   habitat area, when assessing climate change impacts on biodiversity.
   Furthermore, direct estimation of extinction risk incorporates important
   demographic and ecosystem processes, which potentially influence species
   vulnerability to extinction due to climate change.},
