@article{ WOS:000336491600007,
Author = {Bewick, Sharon and Stuble, Katharine L. and Lessard, Jean-Phillipe and
   Dunn, Robert R. and Adler, Frederick R. and Sanders, Nathan J.},
Title = {Predicting future coexistence in a North American ant community},
