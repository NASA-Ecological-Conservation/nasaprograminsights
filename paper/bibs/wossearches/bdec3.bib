Author = {Kelbe, David and van Aardt, Jan and Romanczyk, Paul and van Leeuwen,
   Martin and Cawse-Nicholson, Kerry},
Title = {Multiview Marker-Free Registration of Forest Terrestrial Laser Scanner
   Data With Embedded Confidence Metrics},
Journal = {IEEE TRANSACTIONS ON GEOSCIENCE AND REMOTE SENSING},
Year = {2017},
Volume = {55},
