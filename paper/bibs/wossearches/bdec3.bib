@inproceedings{ WOS:000388114600134,
Author = {Clark, Matthew L.},
Book-Group-Author = {IEEE},
Title = {MAPPING LAND COVER WITH HYPERSPECTRAL AND MULTISPECTRAL SATELLITES USING
   MACHINE LEARNING AND SPECTRAL MIXTURE ANALYSIS},
