@article{ WOS:000385623900020,
Author = {Rifai, Sami W. and Urquiza Munoz, Jose D. and Negron-Juarez, Robinson I.
   and Ramirez Arevalo, Fredy R. and Tello-Espinoza, Rodil and Vanderwel,
