@article{ WOS:000428620000001,
Author = {Nagol, Jyoteshwar R. and Sexton, Joseph O. and Anand, Anupam and
   Sahajpal, Ritvik and Edwards, Thomas C.},
Title = {Isolating type-specific phenologies through spectral unmixing of
   satellite time series},
