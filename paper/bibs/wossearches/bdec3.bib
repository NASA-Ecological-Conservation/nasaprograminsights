@article{ WOS:000430691200004,
Author = {Lukacs, Paul M. and Mitchell, Michael S. and Hebblewhite, Mark and
   Johnson, Bruce K. and Johnson, Heather and Kauffman, Matthew and
   Proffitt, Kelly M. and Zager, Peter and Brodie, Jedediah and Hersey,
