@article{ WOS:000582719500024,
Author = {Guiet, J. and Galbraith, E. D. and Bianchi, D. and Cheung, W. W. L.},
Title = {Bioenergetic influence on the historical development and decline of
   industrial fisheries},
Journal = {ICES JOURNAL OF MARINE SCIENCE},
