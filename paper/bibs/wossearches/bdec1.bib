@article{ WOS:000488771400013,
Author = {Guirado, Emilio and Tabik, Siham and Rivas, Marga L. and Alcaraz-Segura,
   Domingo and Herrera, Francisco},
Title = {Whale counting in satellite and aerial images with deep learning},
