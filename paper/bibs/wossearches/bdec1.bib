@article{ WOS:000687406400010,
Author = {Vaz, Ana Sofia and Selomane, Odirilwe and Jones, Laurence and Gosal,
   Arjan S. and Ziv, Guy and Muller-Karger, Frank Edgar and Montes, Enrique
   and Vallejos, Maria},
Title = {The journey to monitoring ecosystem services: Are we there yet?},
Journal = {ECOSYSTEM SERVICES},
Year = {2021},
Volume = {50},
Month = {AUG},
Abstract = {The Group on Earth Observations Biodiversity Observation Network (GEO
   BON) was established to promote and provide guidance toward a global
   observation network on biodiversity and ecosystems for decision makers
   and the scientific community. Here we comment on three key challenges in
   the development and implementation of monitoring schemes and indicators
   of ecosystem services (ES): (1) combining ES observations, data and
   methods across scales; (2) identifying operational ES metrics that
   consider the interactions between people and ecosys-tems; and (3)
