@article{ WOS:000772224400016,
Author = {Echeverri, Alejandra and Smith, Jeffrey R. and MacArthur-Waltz, Dylan
   and Lauck, Katherine S. and Anderson, Christopher B. and Vargas, Rafael
   Monge and Quesada, Irene Alvarado and Wood, Spencer A. and
   Chaplin-Kramer, Rebecca and Daily, Gretchen C.},
Title = {Biodiversity and infrastructure interact to drive tourism to and within
   Costa Rica},
