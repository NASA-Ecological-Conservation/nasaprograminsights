@article{ WOS:000760639300002,
Author = {Cael, B. B. and Dutkiewicz, Stephanie and Henson, Stephanie},
Title = {Abrupt shifts in 21st-century plankton communities},
Journal = {SCIENCE ADVANCES},
Year = {2021},
Volume = {7},
Number = {44},
Month = {OCT},
Abstract = {Marine microbial communities sustain ocean food webs and mediate global
   elemental cycles. These communities will change with climate; these
   changes can be gradual or foreseeable but likely have much more
   substantial consequences when sudden and unpredictable. In a complex
   virtual marine microbial ecosystem, we find that climate change-driven
   shifts over the 21st century are often abrupt, large in amplitude and
   extent, and unpredictable using standard early warning signals.
   Phytoplankton with unique resource needs, especially fast-growing
   species such as diatoms, are more prone to abrupt shifts. Abrupt shifts
   in biomass, productivity, and community structure are concentrated in
   Atlantic and Pacific subtropics. Abrupt changes in environmental
   variables such as temperature and nutrients rarely precede these
   ecosystem shifts, indicating that rapid community restructuring can
   occur in response to gradual environmental changes, particularly in
   nutrient supply rate ratios.},
