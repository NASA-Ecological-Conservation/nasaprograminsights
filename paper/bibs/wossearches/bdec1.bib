@article{ WOS:000646457900017,
Author = {Zhang, Yanxu and Dutkiewicz, Stephanie and Sunderland, Elsie M.},
Title = {Impacts of climate change on methylmercury formation and bioaccumulation
   in the 21st century ocean},
