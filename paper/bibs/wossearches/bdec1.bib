@article{ WOS:000771734400011,
Author = {Roman, Salvador and Weidberg, Nicolas and Muniz, Carlota and Aguion,
   Alba and Vazquez, Elsa and Santiago, Jose and Seoane, Pablo and
   Barreiro, Berta and Outeiral, Raquel and Villegas-Rios, David and
   Fandino, Susana and Macho, Gonzalo},
Title = {Mesoscale patterns in barnacle reproduction are mediated by
   upwelling-driven thermal variability},
Journal = {MARINE ECOLOGY PROGRESS SERIES},
