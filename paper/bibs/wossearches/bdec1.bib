@article{ WOS:000551465400045,
Author = {Fiechter, Jerome and Santora, Jarrod A. and Chavez, Francisco and
   Northcott, Devon and Messie, Monique},
Title = {Krill Hotspot Formation and Phenology in the California Current
   Ecosystem},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2020},
Volume = {47},
Number = {13},
Month = {JUL 16},
Abstract = {In the California Current Ecosystem, krill represent a key link between
   primary production and higher trophic level species owing to their
   central position in the food web and tendency to form dense
   aggregations. However, the strongly advective circulation associated
   with coastal upwelling may decouple the timing, occurrence, and
   persistence of krill hotspots from phytoplankton biomass and nutrient
   sources. Results from a coupled physical-biological model provide
   insights into fundamental mechanisms controlling the phenology of krill
   hotspots in the California Current Ecosystem, and their sensitivity to
   alongshore changes in coastal upwelling intensity. The simulation
   indicates that dynamics controlling krill hotspot formation, intensity,
   and persistence on seasonal and interannual timescales are strongly
   heterogeneous and related to alongshore variations in
   upwelling-favorable winds, primary production, and ocean currents.
   Furthermore, regions promoting persistent krill hotspot formation
   coincide with increased observed abundance of top predators, indicating
   that the model resolves important ecosystem complexity and function.},
