@article{ WOS:000290678100008,
Author = {Mislan, K. A. S. and Blanchette, Carol A. and Broitman, Bernardo R. and
   Washburn, Libe},
Title = {Spatial variability of emergence, splash, surge, and submergence in
   wave-exposed rocky-shore ecosystems},
Journal = {LIMNOLOGY AND OCEANOGRAPHY},
