@article{ WOS:000298674700008,
Author = {Numata, Izaya and Cochrane, Mark A. and Souza, Jr., Carlos M. and Sales,
   Marcio H.},
Title = {Carbon emissions from deforestation and forest fragmentation in the
   Brazilian Amazon},
