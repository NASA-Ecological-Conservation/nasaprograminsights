@article{ WOS:000328626900005,
Author = {Souza, Jr., Carlos M. and Siqueira, Joao V. and Sales, Marcio H. and
   Fonseca, Antonio V. and Ribeiro, Julia G. and Numata, Izaya and
   Cochrane, Mark A. and Barber, Christopher P. and Roberts, Dar A. and
   Barlow, Jos},
Title = {Ten-Year Landsat Classification of Deforestation and Forest Degradation
   in the Brazilian Amazon},
Journal = {REMOTE SENSING},
Year = {2013},
