@article{ WOS:000306759100001,
Author = {Fricker, Geoffrey A. and Saatchi, Sassan S. and Meyer, Victoria and
   Gillespie, Thomas W. and Sheng, Yongwei},
Title = {Application of Semi-Automated Filter to Improve Waveform Lidar
   Sub-Canopy Elevation Model},
Journal = {REMOTE SENSING},
