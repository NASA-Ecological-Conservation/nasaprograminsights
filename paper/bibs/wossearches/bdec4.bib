@article{ WOS:000771223100001,
Author = {Hu, Chuanmin},
Title = {Hyperspectral reflectance spectra of floating matters derived from
   Hyperspectral Imager for the Coastal Ocean (HICO) observations},
Journal = {EARTH SYSTEM SCIENCE DATA},
