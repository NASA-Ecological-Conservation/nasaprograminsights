@article{ WOS:000364215500021,
Author = {Kahru, Mati and Kudela, Raphael M. and Anderson, Clarissa R. and
   Mitchell, B. Greg},
Title = {Optimized Merger of Ocean Chlorophyll Algorithms of MODIS-Aqua and VIIRS},
Journal = {IEEE GEOSCIENCE AND REMOTE SENSING LETTERS},
Year = {2015},
Volume = {12},
Number = {11},
Pages = {2282-2285},
Month = {NOV},
Abstract = {Standard ocean chlorophyll-a (Chla) products from currently operational
   satellite sensors Moderate Resolution Imaging Spectroradiometer (MODIS)
   Aqua and Visible Infrared Imager Radiometer Suite (VIIRS) underestimate
   medium and high in situ Chla concentrations and have approximately 9\%
   bias between each other in the California Current. By using the regional
   optimization approach of Kahru et al., we minimized the differences
   between satellite estimates and in situ match-ups as well as between
   estimates of the two satellite sensors and created improved empirical
   algorithms for both sensors. The regionally optimized Chla estimates
   from MODIS-Aqua and VIIRS have no bias between each other, have improved
   retrievals at medium to high in situ Chla, and can be merged to improve
   temporal frequency and spatial coverage and to extend the merged time
   series.},
