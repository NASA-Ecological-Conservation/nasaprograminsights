@article{ WOS:000427542100099,
Author = {McCarthy, Matthew J. and Otis, Daniel B. and Mendez-Lazaro, Pablo and
   Muller-Karger, Frank E.},
Title = {Water Quality Drivers in 11 Gulf of Mexico Estuaries},
