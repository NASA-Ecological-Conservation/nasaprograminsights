@article{ WOS:000475570000006,
Author = {Lynch, Maureen A. and Youngflesh, Casey and Agha, Nadia H. and Ottinger,
   Mary Ann and Lynch, Heather J.},
Title = {Tourism and stress hormone measures in Gentoo Penguins on the Antarctic
   Peninsula},
