@article{ WOS:000493735100024,
Author = {Zhu, Likai and Ives, Anthony R. and Zhang, Chi and Guo, Yuanyuan and
   Radeloff, Volker C.},
Title = {Climate change causes functionally colder winters for snow
   cover-dependent organisms},
Journal = {NATURE CLIMATE CHANGE},
Year = {2019},
