Title = {Remote sensing estimation of surface oil volume during the 2010
   Deepwater Horizon oil blowout in the Gulf of Mexico: scaling up AVIRIS
   observations with MODIS measurements},
Journal = {JOURNAL OF APPLIED REMOTE SENSING},
Year = {2018},
Volume = {12},
